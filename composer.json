{
    "name": "eelcol/laravel-html-dom",
    "description": "Laravel HTML Dom implementation",
    "keywords": ["laravel", "html","dom"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eelco Luurtsema",
            "email": "eelcoluurtsema@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8|^9|^10",
        "illuminate/database": "^8|^9|^10",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Eelcol\\LaravelHtmlDom\\": "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eelcol\\LaravelHtmlDom\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eelcol\\LaravelHtmlDom\\HtmlDomServiceProvider"
            ]
         }
     }
}
